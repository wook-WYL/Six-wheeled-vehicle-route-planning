Panels:
  # ... (所有Panel部分保持您原来的设置不变) ...
  - Class: rviz_common/Displays
    Help Height: 0
    Name: Displays
    Property Tree Widget:
      Expanded:
        - /VGraph1/Namespaces1
      Splitter Ratio: 0.8294117450714111
    Tree Height: 859
  - Class: rviz_common/Selection
    Name: Selection
  - Class: rviz_common/Tool Properties
    Expanded:
      - /Goalpoint1
    Name: Tool Properties
    Splitter Ratio: 0.5886790156364441
  - Class: rviz_common/Views
    Expanded:
      - /Current View1
    Name: Views
    Splitter Ratio: 0.5
  - Class: rviz_common/Time
    Experimental: false
    Name: Time
    SyncMode: 0
    SyncSource: FreePaths
  - Class: teleop_rviz_plugin_plus/TeleopPanel
    Name: TeleopPanel
Visualization Manager:
  Class: ""
  Displays:
    - Class: rviz_default_plugins/Axes
      Enabled: true
      Length: 1.5
      Name: Vehicle
      Radius: 0.2
      Reference Frame: vehicle
      Value: true
    - Class: rviz_default_plugins/Image
      Enabled: false # ############ 修改: 默认禁用摄像头画面 ############
      Name: Image
      Topic:
        Value: /camera/color/image_raw
      Value: false
    - Class: rviz_default_plugins/Image
      Enabled: false # ############ 修改: 默认禁用语义图像 ############
      Name: SemanticImage
      Topic:
        Value: /camera/semantic_image
      Value: false
    - Class: rviz_default_plugins/PointCloud2
      Enabled: false # ############ 修改: 默认禁用原始点云 (性能杀手!) ############
      Name: RegScan
      Decay Time: 1.0 # 优化: 添加衰减时间
      Size (Pixels): 2 # 优化: 统一像素大小
      Style: Points
      Topic:
        Value: /camera/depth_registered/points
      Value: false
    - Class: rviz_default_plugins/PointCloud2
      Enabled: false # 默认禁用
      Name: SensorScan
      Size (Pixels): 2
      Style: Points
      Topic:
        Value: /sensor_scan
      Value: false
    - Class: rviz_default_plugins/PointCloud2
      Color Transformer: Intensity
      Decay Time: 2.0 # 优化: 添加衰减时间
      Enabled: true # ############ 核心显示: 默认开启并优化 ############
      Name: TerrainMap
      Size (Pixels): 3 # 优化: 稍大的点
      Style: Points
      Topic:
        Value: /terrain_map
      Value: true
    - Class: rviz_default_plugins/PointCloud2
      Color Transformer: Intensity
      Decay Time: 2.0
      Enabled: false # 默认禁用
      Name: TerrainMapExt
      Size (Pixels): 3
      Style: Points
      Topic:
        Value: /terrain_map_ext
      Value: false
    - Class: rviz_default_plugins/Path
      Color: 25; 255; 0
      Enabled: true
      Name: Path
      Topic:
        Value: /path
      Value: true
    - Class: rviz_default_plugins/PointCloud2
      Color Transformer: Intensity
      Decay Time: 1.0 # 优化: 添加衰减时间
      Enabled: true # ############ 核心显示: 默认开启并优化 ############
      Name: FreePaths
      Size (Pixels): 2
      Style: Points
      Topic:
        Value: /free_paths
      Value: true
    - Class: rviz_default_plugins/MarkerArray
      Enabled: true
      Name: Goalpoint
      Namespaces:
        free_goal: true
        original_goal: true
        waypoint: true
      Topic:
        Value: /viz_node_topic
      Value: true
    # ... (Boundary, AddedObstacles, OverallMap, ExploredAreas, Trajectory 等默认禁用或保持原样) ...
    - Class: rviz_default_plugins/Polygon
      Enabled: false
      Name: Boundary
      Topic:
        Value: /navigation_boundary
      Value: false
    - Class: rviz_default_plugins/PointCloud2
      Enabled: false
      Name: AddedObstacles
      Topic:
        Value: /added_obstacles
      Value: false
    - Class: rviz_default_plugins/PointCloud2
      Enabled: true # OverallMap通常是静态的，开销不大，可以保留
      Name: OverallMap
      Decay Time: 0
      Size (Pixels): 2
      Style: Points
      Topic:
        Value: /overall_map
      Value: true
    - Class: rviz_default_plugins/PointCloud2
      Enabled: false
      Name: ExploredAreas
      Topic:
        Value: /explored_areas
      Value: false
    - Class: rviz_default_plugins/PointCloud2
      Enabled: false
      Name: Trajectory
      Topic:
        Value: /trajectory
      Value: false
    - Class: rviz_default_plugins/Marker
      Enabled: true
      Name: GlobalPath
      Topic:
        Value: /viz_path_topic
      Value: true
    - Class: rviz_default_plugins/MarkerArray
      Enabled: true
      Name: VGraph
      Namespaces:
        freespace_vgraph: true
        polygon_edge: true
      Topic:
        Value: /viz_graph_topic
      Value: true
    - Class: rviz_default_plugins/MarkerArray
      Enabled: true
      Name: ViewpointExtend
      Namespaces:
        corner_direct: true
        extend_viewpoint: true
        origin_viewpoint: true
        raytracing_line: true
      Topic:
        Value: /viz_viewpoint_extend_topic
      Value: true
  Enabled: true
  Global Options:
    Background Color: 0; 0; 0
    Fixed Frame: map
    Frame Rate: 15
  Name: root
  Tools:
    # ... (所有Tools部分保持您原来的设置不变) ...
    - Class: rviz_default_plugins/MoveCamera
    - Class: goalpoint_rviz_plugin/GoalpointTool
      Topic:
        Value: goalpoint
  Transformation:
    Current:
      Class: rviz_default_plugins/TF
  Value: true
  Views:
    Current:
      Class: rviz_default_plugins/Orbit
      Distance: 60.17988204956055
      Focal Point:
        X: 11.870485305786133
        Y: 13.185401916503906
        Z: -0.18328675627708435
      Name: Current View
      Near Clip Distance: 0.01
      Pitch: 1.5697963237762451
      Target Frame: map
      Value: Orbit (rviz)
      Yaw: 4.715415000915527
    Saved: ~
