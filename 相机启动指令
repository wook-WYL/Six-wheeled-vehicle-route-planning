ros2 run orbbec_rgb_publisher orbbec_rgb_node
ros2 run orbbec_rgb_publisher orbbec_depth_node
ros2 run orbbec_rgb_publisher orbbec_combined_node



ros2 launch orbbec_camera gemini_330_series.launch.py


ros2 launch orbbec_camera gemini_330_series.launch.py enable_colored_point_cloud:=true enable_sync_output_accel_gyro:=true

ros2 launch orbbec_camera gemini_330_series.launch.py depth_registration:=true enable_sync_output_accel_gyro:=true





source install/setup.bash

ros2 launch robot_bringup 
